<script>
  window.timeblock_path = "<%= timeblock_path(@conn, :index) %>";
  window.assignment_id = "<%= @assignment.id %>";
</script>
<h2>Working Times</h2>

 <ul>
<%= for timeblock <- @assignment.timeblocks do %>
  <li>
    <%= timeblock.start %> |
    <%= timeblock.end %>
  </li>
<% end %>
</ul>

<span><%= link "Edit", to: assignment_path(@conn, :edit, @assignment) %></span>
<span><%= link "Back", to: assignment_path(@conn, :index) %></span>
